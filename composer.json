{
    "name": "geardev/prometheus",
    "description": "Prometheus package for Laravel and geardev.io",
    "autoload": {
        "psr-4": {
            "GearDev\\Prometheus\\": "src/"
        }
    },
    "require": {
        "promphp/prometheus_client_php": "^2.8",
        "geardev/coroutines": "^0.0",
        "geardev/processes": "^0.0",
        "swow/swow": "v1.5.2"
    },
    "require-dev": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "GearDev\\Prometheus\\GearPrometheusProvider"
            ]
        }
    }
}
